{
  "kind": "collectionType",
  "collectionName": "tour_dates",
  "info": {
    "singularName": "tour-date",
    "pluralName": "tour-dates",
    "displayName": "Tour Date"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tour": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tour.tour"
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "displayLabel": {
      "type": "string",
      "required": true
    },
    "available": {
      "type": "boolean",
      "default": true
    },
    "maxBookings": {
      "type": "integer"
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}

