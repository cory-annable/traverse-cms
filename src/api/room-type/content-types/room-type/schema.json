{
  "kind": "collectionType",
  "collectionName": "room_types",
  "info": {
    "singularName": "room-type",
    "pluralName": "room-types",
    "displayName": "Tour Room Type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tour": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tour.tour"
    },
    "roomType": {
      "type": "string",
      "required": true
    },
    "displayLabel": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "currency": {
      "type": "string",
      "default": "NZD"
    },
    "available": {
      "type": "boolean",
      "default": true
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}

